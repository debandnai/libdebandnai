<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:state_enabled="true"
        android:drawable="@drawable/time_slot_checked" />
    <item android:state_pressed="true" android:state_enabled="true"
        android:drawable="@drawable/time_slot_unchecked2" />
    <item android:state_pressed="false" android:state_enabled="true"
        android:drawable="@drawable/time_slot_unchecked2" />
    <item android:state_enabled="false" android:drawable="@drawable/time_slot_blocked2"/>
    <item android:drawable="@drawable/time_slot_blocked2" />

</selector>