<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
    </data>
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <RelativeLayout
        android:id="@+id/itemLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/rl_padding_item_15dp"
        tools:ignore="UselessParent">

        <ImageView
            android:id="@+id/allergensCheckUncheckIv"
            android:layout_width="@dimen/iv_allergen_checkUncheck_width_height_20dp"
            android:layout_height="@dimen/iv_allergen_checkUncheck_width_height_20dp"
            android:layout_centerVertical="true"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/allergensNameTv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/add_child_allergen_text_marginLeft"
            android:layout_toEndOf="@+id/allergensCheckUncheckIv"
            android:text=""
            android:textColor="@color/add_children_colorBlack"
            android:textSize="@dimen/tv_size_allergen_text_13sp" />

    </RelativeLayout>
</RelativeLayout>
</layout>